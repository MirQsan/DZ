    "def tolst(lens, fill):\n",
    "    def decorator(func):\n",
    "        def wrapper(*args):\n",
    "            result = func(*args)\n",
    "            if lens <= len(result):\n",
    "                return result[:lens]\n",
    "            else:\n",
    "                return result + [fill] * (lens - len(result))\n",
    "        return wrapper\n",
    "    return decorator\n",
    "\n",
    "@tolst(5, 0)\n",
    "def my_func():\n",
    "    return [1, 2, 3]\n",
    "\n",
    "print(my_func()) #[1, 2, 3, 0, 0]\n",
    "\n",
    "@tolst(3, 'a')\n",
    "def my_func2():\n",
    "    return [1, 2, 3, 4]\n",
    "\n",
    "print(my_func2()) #[1, 2, 3]"
   ]
